<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

  <threadsafe>true</threadsafe>
  <sessions-enabled>false</sessions-enabled>
  <runtime>java8</runtime>

  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
  </system-properties>
  
  <automatic-scaling>
	<min-idle-instances>0</min-idle-instances>
  </automatic-scaling>
  
  <static-files>
  <include path="/**.html" expiration="1m" />
  <include path="/**.css" expiration="1m" />
  <include path="/**.js" expiration="1m" />
  <include path="/**.jpg" expiration="1m" />
  <include path="/**.gif" expiration="1m" />
  <include path="/**.png" expiration="1m" />
  <include path="/**.mp3" expiration="1m" />
  <include path="/**.wav" expiration="1m" />
  <include path="/**.ttf" expiration="1m" />
  <include path="/**.ico" expiration="1m" />
  <include path="/**.cur" expiration="1m" />
  </static-files>

</appengine-web-app>