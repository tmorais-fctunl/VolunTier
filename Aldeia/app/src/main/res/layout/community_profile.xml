<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.community.CommunityProfileActivity">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineDown"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.9" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineUp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.03" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/profileGuidelineLeft"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.07" />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/profileGuidelineRight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.93" />


        <TextView
            android:id="@+id/community_lookUp_Address"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/profile_Address"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/textView19"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_Address2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/profile_Address2"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/textView20"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_Region"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/profile_Region"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/textView21"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_Pc"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/profile_ZipCode"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/textView26"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_Website"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/profile_Website"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/textView25"
            tools:visibility="visible" />

        <ImageView
            android:id="@+id/community_lookUp_ImageView"
            android:layout_width="200px"
            android:layout_height="200px"
            android:adjustViewBounds="true"
            android:scaleType="fitXY"
            android:src="@drawable/ic_profile"
            android:visibility="invisible"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toTopOf="@+id/guidelineUp"
            app:srcCompat="@drawable/ic_profile"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_Username"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:clickable="false"
            android:ems="10"
            android:focusable="false"
            android:hint="@string/register_username"
            android:textSize="24sp"
            android:textStyle="bold"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="@id/profileGuidelineRight"
            app:layout_constraintStart_toEndOf="@+id/community_lookUp_ImageView"
            app:layout_constraintTop_toTopOf="@+id/guidelineUp"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_Email"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:clickable="false"
            android:ems="10"
            android:focusable="false"
            android:hint="@string/Email"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/textView9"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_Facebook"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/profile_Facebook"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/textView24"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_Instagram"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/profile_Instagram"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/textView23"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_Twitter"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/profile_Twitter"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/textView22"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_FullName"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/profile_FullName"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/textView16"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_Landline"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/profile_LandLine"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/textView17"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_Mobile"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/profile_Mobile"
            android:textSize="16sp"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/textView18"
            tools:visibility="visible" />

        <TextView
            android:id="@+id/community_lookUp_PrivacyPublic"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:textSize="16sp"
            android:text="@string/profile_PrivacyPublic"
            android:visibility="invisible"
            app:layout_constraintEnd_toStartOf="@id/profileGuidelineRight"
            app:layout_constraintStart_toEndOf="@+id/community_lookUp_ImageView"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_Username"
            tools:visibility="visible" />

        <Button
            android:id="@+id/community_lookUp_cancel"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/maps_event_cancel"
            app:layout_constraintBottom_toTopOf="@+id/guidelineDown"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_Twitter" />

        <ProgressBar
            android:id="@+id/com_prof_load_bar"
            style="?android:attr/progressBarStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/guidelineDown"
            app:layout_constraintEnd_toStartOf="@+id/profileGuidelineRight"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toTopOf="@+id/guidelineUp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:textSize="12sp"
            android:text="@string/Email"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_ImageView" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:textSize="12sp"
            android:text="@string/profile_FullName"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_Email" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/profile_LandLine"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_FullName" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/profile_Mobile"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_Landline" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/profile_Address"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_Mobile" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:textSize="12sp"
            android:text="@string/profile_Address2"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_Address" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/profile_Region"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_Address2" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/profile_Twitter"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_Instagram" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/profile_Instagram"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_Facebook" />

        <TextView
            android:id="@+id/textView24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/profile_Facebook"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_Website" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="@string/profile_Website"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_Pc" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:layout_marginTop="16dp"
            android:text="@string/profile_ZipCode"
            app:layout_constraintStart_toStartOf="@+id/profileGuidelineLeft"
            app:layout_constraintTop_toBottomOf="@+id/community_lookUp_Region" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>